{
	"name": "api",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "node index.js",
		"dev": "concurrently --kill-others-on-fail 'yarn dev:db' 'yarn dev:ts-node'",
		"dev:db": "docker-compose up",
		"dev:ts-node": "wait-on tcp:5432 --interval 1000 && prisma generate && prisma migrate dev && nodemon index.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix --quiet",
		"check-types": "tsc --noEmit",
		"postinstall": "prisma generate"
	},
	"devDependencies": {
		"@types/ms": "0.7.31",
		"@types/node": "18.11.8",
		"eslint-config-devfaq": "workspace:*",
		"pino-pretty": "9.1.1",
		"tsconfig": "workspace:*",
		"typescript": "4.8.4"
	},
	"dependencies": {
		"@fastify/cookie": "8.3.0",
		"@fastify/oauth2": "6.1.0",
		"@fastify/sensible": "5.1.1",
		"@fastify/session": "10.0.2",
		"@fastify/swagger": "8.1.0",
		"@fastify/swagger-ui": "1.1.0",
		"@fastify/type-provider-typebox": "2.3.0",
		"@prisma/client": "4.5.0",
		"@sinclair/typebox": "0.25.2",
		"@swc/core": "1.3.11",
		"concurrently": "7.5.0",
		"fastify": "4.9.2",
		"fastify-plugin": "4.3.0",
		"lru-cache": "7.14.0",
		"ms": "2.1.3",
		"nodemon": "2.0.20",
		"prisma": "4.5.0",
		"ts-node": "10.9.1",
		"undici": "5.12.0",
		"wait-on": "6.0.1"
	}
}
